{
  "name": "ORVA",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "yarn android:dev",
    "android:v2": "yarn android:dev:v2",
    "android_mobile": "yarn android --device Pixel_5_API_34(AVD) - 14",
    "android_tv": "yarn android --device Television_1080p_API_31(AVD) - 12",
    "android:dev": "react-native run-android --mode=developmentDebug",
    "android:dev:v2": "react-native run-android --mode=v2developmentDebug",
    "android:uaestag": "react-native run-android --mode=uaestagingDebug",
    "android:usstag": "react-native run-android --mode=usstagingDebug",
    "android:uaeprod": "react-native run-android --mode=uaeproductionRelease",
    "android:usprod": "react-native run-android --mode=usproductionDebug",
    "buildDevApk": "cd android && ./gradlew clean && ./gradlew assembleDevelopmentRelease && cd ..",
    "buildDevV2Apk": "cd android && ./gradlew clean && ./gradlew assembleV2developmentRelease && cd ..",
    "buildUsStagApk": "cd android && ./gradlew clean && ./gradlew assembleUsstagingRelease && cd ..",
    "buildUaeStagApk": "cd android && ./gradlew clean && ./gradlew assembleUaestagingRelease && cd ..",
    "buildUsProdApk": "cd android && ./gradlew clean && ./gradlew assembleUsproductionRelease && cd ..",
    "buildUaeProdApk": "cd android && ./gradlew clean && ./gradlew assembleUaeproductionRelease && cd ..",
    "resetAndroid": "cd android && ./gradlew clean && cd .. && yarn run android",
    "resetIos": "cd ios && pod deintegrate && rm -rf Podfile.lock && pod install && cd .. && yarn run ios",
    "ios": "yarn ios:dev",
    "ios:dev": "react-native run-ios --scheme=ORVADevelopment --simulator=\"iPhone 15 Pro\"",
    "ios:stag": "react-native run-ios --scheme=ORVAStaging --simulator=\"iPhone 15 Pro\"",
    "ios:uaeprod": "react-native run-ios --scheme=ORVAUaeProduction --simulator=\"iPhone 15 Pro\"",
    "ios:usprod": "react-native run-ios --scheme=ORVAUsProduction --simulator=\"iPhone 15 Pro\"",
    "lint": "eslint .",
    "start": "react-native start",
    "start:debug": "react-native start --experimental-debugger",
    "test": "jest"
  },
  "dependencies": {
    "@react-native-community/blur": "^4.4.1",
    "@react-native-community/datetimepicker": "^8.2.0",
    "@react-native-community/netinfo": "^11.3.2",
